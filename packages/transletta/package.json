{
  "name": "transletta",
  "version": "0.0.0",
  "type": "module",
  "description": "Elegant i18n build system for humans",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/config.d.ts",
      "import": "./dist/config/config.js",
      "require": "./dist/config/config.js"
    },
    "./plugin": {
      "types": "./dist/plugins/plugin.d.ts",
      "import": "./dist/plugins/plugin.js",
      "require": "./dist/plugins/plugin.js"
    },
    "./next": {
      "types": "./dist/frameworks/next.d.ts",
      "import": "./dist/frameworks/next.js",
      "require": "./dist/frameworks/next.js"
    },
    "./vite": {
      "types": "./dist/frameworks/vite.d.ts",
      "import": "./dist/frameworks/vite.js",
      "require": "./dist/frameworks/vite.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/twlite/transletta.git"
  },
  "author": "Twilight <hello@twlite.dev>",
  "homepage": "https://transletta.twlite.dev",
  "keywords": [
    "i18n",
    "translation",
    "localization",
    "globalization",
    "internationalization"
  ],
  "devDependencies": {
    "@types/node": "catalog:build",
    "tsup": "catalog:build",
    "typescript": "catalog:build"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "smol-toml": "^1.4.2"
  }
}